<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Proyectoprogreso2.RegistrarsePage"
             Title="RegistrarsePage">
    <VerticalStackLayout>
        <Image
Source="data:image/png;base64,"
HeightRequest="50"
HorizontalOptions="Fill" BackgroundColor="Red"/>

        <HorizontalStackLayout HorizontalOptions="Center">

            <Label 
 FontAttributes="Bold"
 Text="Registrarse"
 FontSize="30"
 VerticalOptions="Center" 
 HorizontalOptions="Center" 
 TextColor="Red"/>
        </HorizontalStackLayout>
        <TableView>
            <TableRoot>
                <TableSection Title="Regístrate" TextColor="Red"  >

                    <EntryCell x:Name="ID"  
  Placeholder="Cédula"
  />
                    <EntryCell x:Name="Nombre"  
  Placeholder="Nombre"
  />

                    <EntryCell x:Name="Apellido"  
  Placeholder="Apellido"
  />
                    <EntryCell x:Name="Direccion"  
  Placeholder="Dirección"
  />
                    <EntryCell x:Name="Tarjeta"  
  Placeholder="Número de tarjeta"
  />
                    
                    <EntryCell x:Name="NombreU"
                       
             Placeholder="Nombre de Usuario"/>
                    <EntryCell x:Name="Contraseña"  
                     Placeholder="Contraseña"
                     />

                </TableSection>
            </TableRoot>
        </TableView>
        <HorizontalStackLayout HorizontalOptions="Center">
            <Button Text="Iniciar Sesión"
    BackgroundColor="Red" CornerRadius="50" Clicked="OnClickLogin"/>

            <Label Text="         "/>

            <Button Text="Registrarse" Clicked="OnClickRegistrarse"
    BackgroundColor="Black" CornerRadius="50"/>

        </HorizontalStackLayout>
    </VerticalStackLayout>
</ContentPage>